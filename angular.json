{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "elements": {
      "root": "projects/elements/",
      "sourceRoot": "projects/elements/src",
      "projectType": "application",
      "prefix": "greeter",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "ngx-build-plus:build",
          "options": {
            "outputPath": "dist/elements",
            "index": "projects/elements/src/index.html",
            "main": "projects/elements/src/main.ts",
            "polyfills": "projects/elements/src/polyfills.ts",
            "tsConfig": "projects/elements/tsconfig.app.json",
            "assets": [
              "projects/elements/src/favicon.ico",
              "projects/elements/src/assets"
            ],
            "styles": [
              "projects/elements/src/styles.css"
            ],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/elements/src/environments/environment.ts",
                  "with": "projects/elements/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "elements:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "elements:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "elements:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/elements/src/test.ts",
            "polyfills": "projects/elements/src/polyfills.ts",
            "tsConfig": "projects/elements/tsconfig.spec.json",
            "karmaConfig": "projects/elements/karma.conf.js",
            "styles": [
              "projects/elements/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/elements/src/favicon.ico",
              "projects/elements/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/elements/tsconfig.app.json",
              "projects/elements/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "push": {
      "root": "projects/push",
      "sourceRoot": "projects/push/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/push/tsconfig.lib.json",
            "project": "projects/push/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/push/src/test.ts",
            "tsConfig": "projects/push/tsconfig.spec.json",
            "karmaConfig": "projects/push/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/push/tsconfig.lib.json",
              "projects/push/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "element-integration": {
      "root": "projects/element-integration/",
      "sourceRoot": "projects/element-integration/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/element-integration",
            "index": "projects/element-integration/src/index.html",
            "main": "projects/element-integration/src/main.ts",
            "polyfills": "projects/element-integration/src/polyfills.ts",
            "tsConfig": "projects/element-integration/tsconfig.app.json",
            "assets": [
              "projects/element-integration/src/favicon.ico",
              "projects/element-integration/src/assets"
            ],
            "styles": [
              "projects/element-integration/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/element-integration/src/environments/environment.ts",
                  "with": "projects/element-integration/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "element-integration:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "element-integration:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "element-integration:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/element-integration/src/test.ts",
            "polyfills": "projects/element-integration/src/polyfills.ts",
            "tsConfig": "projects/element-integration/tsconfig.spec.json",
            "karmaConfig": "projects/element-integration/karma.conf.js",
            "styles": [
              "projects/element-integration/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/element-integration/src/favicon.ico",
              "projects/element-integration/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/element-integration/tsconfig.app.json",
              "projects/element-integration/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "workspace"
}
